<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>klang</groupId>
  <artifactId>klang</artifactId>
  <version>0.0.8</version>
  <name>klang</name>
  <description>k library</description>
  <distributionManagement>
      <!-- use mvn -U deploy -DgeneratePom=true -Dpackaging=jar  -->
      <repository>
          <id>artifactory</id>
          <name>releases</name>
          <url>http://europambee-build:8082/artifactory/libs-release-local</url>
      </repository>
  </distributionManagement>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source/>
          <target/>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
      <dependency>
          <groupId>org.antlr</groupId>
          <artifactId>antlr4</artifactId>
          <version>4.5</version>
      </dependency>
      
            <dependency>
                <groupId>Klangjar</groupId>
                <artifactId>klangjar</artifactId>
                <version>1.0</version>
                <scope>system</scope>
                <type>jar</type>
                <systemPath>${project.basedir}/klang.jar</systemPath>
            </dependency>
            <dependency>
                <groupId>com.microsoft.z3</groupId>
                <artifactId>com.microsoft.z3</artifactId>
                <version>1.0</version>
                <scope>system</scope>
                <type>jar</type>
                <systemPath>${project.basedir}/lib/com.microsoft.z3.jar</systemPath>
            </dependency>
            <dependency>
                <groupId>com.microsoft.z3</groupId>
                <artifactId>libz3java</artifactId>
                <version>1.0</version>
                <scope>system</scope>
                <type>so</type>
                <systemPath>${project.basedir}/lib/libz3java.so</systemPath>
            </dependency>
            <dependency>
                <groupId>com.microsoft.z3</groupId>
                <artifactId>libz3</artifactId>
                <version>1.0</version>
                <scope>system</scope>
                <type>so</type>
                <systemPath>${project.basedir}/lib/libz3.so</systemPath>
            </dependency>

